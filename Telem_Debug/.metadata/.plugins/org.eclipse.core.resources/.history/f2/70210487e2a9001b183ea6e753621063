#include <msp430.h> 

/*
 * main.c
 */
int main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer
    P1DIR |= BIT0;                            // P1.0 output
    P11DIR |= 0x07;                           // ACLK, MCLK, SMCLK set out to pins
    P11SEL |= 0x07;                           // P11.0,1,2 for debugging purposes.

    while(1)
    {
      P1OUT ^= BIT0;                          // Toggle P1.0
      __delay_cycles(60000);                  // Delay
    }

	return 0;
}
